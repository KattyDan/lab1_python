n=int (input ("число n")) #запрос числа у пользователя и преобразование его в целое int
ob=[] #обратный порядок
while n>1: 
	ob+=[n] #добавление текущего значение n в список ob
	n=n-1 #уменьшение n на 1
	if n==1: #если n стало равно 1
		ob+=[n] #добавление 1 в список
print (ob) #вывод получившегося списка

n=int (input ("чило n"))
fa=1 #переменная для хранени факториала
while n>0:
	fa*=n #умножение fa на текущее значение n
	n-=1 #уменьшаем n на 1
print (fa) #вывод fa числа n
